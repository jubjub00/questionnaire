                                android:fontFamily="@font/kanit"
                                android:text="ไม่มี"
                                android:onClick="onRadioButtonClicked"
                                android:textSize="15sp" />
                        </RadioGroup>
                    </androidx.constraintlayout.widget.ConstraintLayout>
